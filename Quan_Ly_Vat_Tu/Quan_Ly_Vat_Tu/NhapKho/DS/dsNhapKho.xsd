          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="CHUNG_TU_Theo_NgayTableAdapter" GeneratorDataComponentClassName="CHUNG_TU_Theo_NgayTableAdapter" Name="CHUNG_TU_Theo_Ngay" UserDataComponentName="CHUNG_TU_Theo_NgayTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="Quan_Ly_Vat_TuConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--DECLARE @FromDate DATETIME
--DECLARE @ToDate DATETIME
--
--SET @FromDate = '1/1/2000'
--SET @ToDate = '12/31/2017'

SELECT ct.Ma_Chung_Tu,
       ct.Ngay,
       ct.Ma_Hoa_Don,
       ct.Loai_Chung_Tu,
       ct.Ma_Khach_Hang,
       kh.Ten_Khach_Hang,
       ct.Dia_Chi,
       ct.Ly_Do,
       ct.VAT_Phan_Tram,
       ct.VAT_Thanh_Tien,
       ct.Chiet_Khau_Phan_Tram,
       ct.Chiet_Khau_Thanh_Tien,
       ct.Thanh_Tien,
       ct.Ghi_Chu,
       ct.So_Thu_Tu,
       nv.Ho_Ten,
       ct.Hinh_Thuc_Thu_Chi,
       ct.Hinh_Thuc_Cong_No,
       ct.Thanh_Toan,
       CASE 
            WHEN ct.Hinh_Thuc_Thu_Chi = 0 THEN (
                     CASE 
                          WHEN ct.Hinh_Thuc_Cong_No = 1 THEN (ct.Thanh_Tien - ISNULL(ct.Thanh_Toan, 0))
                          WHEN ct.Hinh_Thuc_Cong_No = 2 THEN ((ct.Thanh_Tien - ISNULL(ct.Thanh_Toan, 0)) * -1)
                          ELSE 0
                     END +
                     ISNULL(
                         (
                             SELECT SUM(
                                        CASE 
                                             WHEN tc.Hinh_Thuc_Cong_No = 1 THEN 
                                                  (tc.So_Tien)
                                             ELSE (tc.So_Tien * -1)
                                        END
                                    )
                             FROM   THU_CHI tc
                             WHERE  tc.Chung_Tu_Ban_Hang = ct.Ma_Chung_Tu
                         ),
                         0
                     )
                 )
       END AS Cong_No_Con_Lai,
       (
           SELECT SUM(
                      CASE 
                           WHEN tc.Hinh_Thuc_Cong_No = 1 THEN tc.So_Tien
                           ELSE (tc.So_Tien * -1)
                      END
                  )
           FROM   THU_CHI tc
           WHERE  tc.Chung_Tu_Ban_Hang = ct.Ma_Chung_Tu
       ) AS Da_Thu_Chi
FROM   CHUNG_TU ct
       LEFT OUTER JOIN KHACH_HANG kh
            ON  kh.Ma_Khach_Hang = ct.Ma_Khach_Hang
       LEFT OUTER JOIN NHAN_VIEN nv
            ON  nv.Ma_Nhan_Vien = ct.Nhan_Vien
WHERE  ct.Loai_Chung_Tu = 1
       AND DATEDIFF(DAY, @FromDate, ct.Ngay) &gt;= 0
       AND DATEDIFF(DAY, @ToDate, ct.Ngay) &lt;= 0</CommandText>
